  <ThingDef ParentName="BaseFilth">
    <defName>Filth_FrostdustBlue</defName>
    <label>blue frozen matter</label>
    <!-- <thingClass>ThingWithComps</thingClass> -->
    <description>A puddle of sticky freezing chemfuel.</description>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <Beauty>-11</Beauty>
      <Cleanliness>-15</Cleanliness>
      <MaxHitPoints>0</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Spatter</texPath>
      <color>(127, 192, 255, 127)</color>
    </graphicData>
    <filth>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>25</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <maxThickness>3</maxThickness>
    </filth>
    <pathCost>3</pathCost>
