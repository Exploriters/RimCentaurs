      </li>
      <li Class="Explorite.CompProperties_EnsureAbility" MayRequire="Ludeon.RimWorld.Royalty">
        <abilities>
          <li MayRequire="Ludeon.RimWorld.Royalty">MassPsychicDeafCentaur</li>
        </abilities>
      </li>
    </comps>
