	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/PreceptDef[(
				   defName="Biosculpting_Accelerated"
				)and not (requiredMemes)]</xpath>
		<value>
			<requiredMemes />
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<success>Always</success>
		<xpath>Defs/PreceptDef[defName="Biosculpting_Accelerated"]
				/requiredMemes</xpath>
		<value>
			<li>Centaur</li>
		</value>
	</Operation>
	
