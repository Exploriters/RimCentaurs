<Defs>
  
  <ThingDef ParentName="ApparelArmorHelmetReconBase">
    <defName>Apparel_ArmorHelmetReconClocked</defName>
    <tradeability>Sellable</tradeability>
    <label>clocked recon helmet</label>
    <description>A high-tech armored helmet with a built-in status computer and a protective plasteel-weave plate.\n\nThis modified verison is invisible, powered by clocking shield technology, but behaviored as a normal helmet and requires more skills to make, allowing someone showing it best face and hair without the thread of exposing head.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/PrestigeReconHelmet/PrestigeReconHelmet</texPath>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(0.9, 0.9)</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>31500</WorkToMake>
    </statBases>
    <equippedStatOffsets>
      <PsychicSensitivity ExEx="Royalty">0.05</PsychicSensitivity>
      <PsychicEntropyRecoveryRate ExEx="Royalty">0.033</PsychicEntropyRecoveryRate>
    </equippedStatOffsets>
    <recipeMaker>
      <skillRequirements>
        <Crafting>9</Crafting>
      </skillRequirements>
	  <researchPrerequisites>
		<li>ReconArmor</li>
		<li>CentaurReverseEngineeringHumanTech</li>
	  </researchPrerequisites>
    </recipeMaker>
    <costList Inherit="False">
      <ComponentSpacer>1</ComponentSpacer>
      <Plasteel>40</Plasteel>
      <Gold>4</Gold>
    </costList>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <wearPerDay>0</wearPerDay>
      <tags>
        <li>PrestigeCombatGear</li>
      </tags>
      <wornGraphicPath isNull="True" />
      <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
    <colorGenerator Inherit="False" Class="ColorGenerator_Options">
      <options>
        <li>
          <only>(0.24313725490196078431372549019608,0.4,0.70980392156862745098039215686275)</only>
        </li>
      </options>
    </colorGenerator>
    <thingCategories Inherit="False">
      <li>CentaurApparels</li>
    </thingCategories>
  </ThingDef>

