      <li>ExoticMisc</li>
    </tradeTags>
    <tradeability>Sellable</tradeability>
    <statBases>
      <ShootingAccuracyTurret>0.95</ShootingAccuracyTurret>
    </statBases>
    <costList Inherit="False">
      <ComponentSpacer>300</ComponentSpacer>
      <ThermoDamper>1</ThermoDamper>
    </costList>
